  "settings": [
    {
      "name": "accessKeyId",
      "viewName": "Access key",
      "type": "vault",
      "description": "The access key ID of the default user to use to authenticate to AWS."
    },
    {
      "name": "secretAccessKey",
      "viewName": "Secret key",
      "type": "vault",
      "description": "The access key secret of the default user to use to authenticate to AWS."
    },
    {
      "name": "region",
      "viewName": "Region",
